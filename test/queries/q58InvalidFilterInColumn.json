{
    "title": "SELECT dept, avg WHERE avg > 70",
    "query": {
        "WHERE": {
            "AND":[
                {
                    "GT": {
                        "courses_avg": 90
                    }
                },
                {
                    "IS": {
                        "courses_dept": "cpsc"
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "courses_title",
                "courses_uuid",
                "overallAvg"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "overallAvg"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "courses_title"
            ],
            "APPLY": [
                {
                    "overallAvg": {
                        "AVG": "courses_avg"
                    }
                }
            ]
        }
    },
    "response": {
        "code": 400,
        "body": {
            "error": ""
        }
    }
}
